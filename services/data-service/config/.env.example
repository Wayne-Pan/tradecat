# ============================================================
# data-service 私有配置
# 公共配置（数据库、代理、币种）在 config/.env
# ============================================================

# ---------- 数据补齐 ----------
# 补齐模式: none(不补) / days(最近N天) / all(全部历史)
BACKFILL_MODE=days
# 补齐天数（days模式生效）
BACKFILL_DAYS=30
# 启动时自动补齐
BACKFILL_ON_START=false

# ---------- 采集限流 ----------
# 每分钟最大请求数
RATE_LIMIT_PER_MINUTE=1800
# 最大并发数
MAX_CONCURRENT=5

# ---------- WebSocket ----------
# 缺口检测间隔（秒）
BINANCE_WS_GAP_INTERVAL=600
# 数据源标识
BINANCE_WS_SOURCE=binance_ws
